<div class="modal fade" id="editarModal" tabindex="-1" role="dialog" aria-labelledby="editarModal" aria-hidden="true">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="editarModal">Cambiar datos del usuario</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <form action="/administradorActualizar" method="POST">

                <div class="modal-body">
                    <input name="cuentaid" id="cuentaid" type="text" class="form-control"
                        value="<%- admincuenta._id %>">
                    <div class="form-group">
                        <label for="email">Su correo electronico</label>
                        <input value="<%- admincuenta.email %>" type="email" name="email" class="form-control"
                            id="email" required>
                    </div>
                    <div class="form-group">
                        <label for="password">Cree una contrase√±a</label>
                        <input value="<%- admincuenta.password %>" type="text" name="password" class="form-control"
                            id="password" required>
                    </div>
                    <div class="form-group">
                        <label for="cuit">CUIT</label>
                        <input value="<%- admincuenta.cuit %>" type="number" name="cuit" class="form-control" id="cuit"
                            required>

                    </div>
                    <div class="form-group">
                        <label for="numerodecelular">Numero de celular</label>
                        <input value="<%- admincuenta.numerodecelular %>" type="text" name="numerodecelular"
                            class="form-control" id="numerodecelular" required>
                    </div>
                    <div class="form-group">
                        <label for="dniadelante">Foto de la parte de adelante de su DNI</label>
                        <div class="custom-file">
                            <input type="file" name="dni" class="custom-file-input" id="customFileLang2" lang="es">
                            <label class="custom-file-label" for="customFileLang2">Seleccionar Archivo</label>
                        </div>
                    </div>
                    <div class="form-group">
                        <label for="dniatras">Foto de la parte de atras de su DNI</label>
                        <div class="custom-file">
                            <input type="file" name="dni1" class="custom-file-input" id="customFileLang" lang="es">
                            <label class="custom-file-label" for="customFileLang">Seleccionar Archivo</label>
                        </div>
                    </div>

                </div>
                <div class="modal-footer">
                    <div class="form-group">
                        <button type="submit" class="btn btn-danger">Aplicar cambios</button>
                        <button type="button" class="btn btn-secondary" data-dismiss="modal">Cerrar</button>
                    </div>
            </form>
        </div>
    </div>
</div>